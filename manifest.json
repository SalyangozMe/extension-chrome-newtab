{
  "name": "Salyangoz New Tab: View what's new on Salyangoz",
  "short_name": "Salyangoz New Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "View what's new on Salyangoz",
  "homepage_url": "http://salyangoz.me",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions" : [
    "webRequest",
    "webRequestBlocking",
    "*://salyangoz.me/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides" : {
    "newtab": "src/newtab.html"
  }
 }
